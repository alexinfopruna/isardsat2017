function icl_retry_mtr(a){var id=a.getAttribute('id');spl=id.split('_');var loc=location.href.replace(/#(.*)$/,'').replace(/(&|\?)(retry_mtr)=([0-9]+)/g,'').replace(/&nonce=([0-9a-z]+)(&|$)/g,'');if(-1==loc.indexOf('?')){url_glue='?';}else{url_glue='&';}
